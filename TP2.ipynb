    "### d\n",
    "\n",
    "# calculate the covariance matrix\n",
    "\n",
    "rows_qty = x.shape[0]\n",
    "col_qty = x.shape[1]\n",
    "\n",
    "cov = np.zeros(shape=(col_qty, col_qty))\n",
    "\n",
    "for i in range(col_qty):\n",
    "    for j in range(col_qty):\n",
    "        i_col_mean = np.mean(x[:, i])\n",
    "        j_col_mean = np.mean(x[:, j])\n",
